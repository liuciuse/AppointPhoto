<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" android:background="@color/default_bg_color" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <include android:id="@id/nav_bar" layout="@layout/navbar" />
    <TextView android:layout_gravity="bottom" android:background="@color/upload_work_edittext_color" android:layout_width="fill_parent" android:layout_height="@dimen/line_height" android:layout_marginTop="@dimen/select_item_category_margin_top" />
    <RelativeLayout android:background="@color/white" android:layout_width="fill_parent" android:layout_height="@dimen/menu_item_child_height">
        <TextView android:textSize="@dimen/menu_user_name_textsize" android:textColor="@color/home_user_name_color" android:gravity="center_vertical" android:layout_gravity="left" android:id="@id/item_book_contact_user_name_textV" android:background="@color/white" android:paddingLeft="@dimen/general_space" android:layout_width="wrap_content" android:layout_height="@dimen/menu_item_child_height" android:text="联系人：" />
        <EditText android:textSize="@dimen/menu_user_name_textsize" android:textColor="@color/home_user_name_color" android:textColorHint="@color/upload_work_edittext_color" android:id="@id/item_name_editText" android:background="@color/white" android:layout_width="fill_parent" android:layout_height="@dimen/menu_item_child_height" android:hint="请输入联系人" android:singleLine="true" android:layout_toRightOf="@id/item_book_contact_user_name_textV" />
    </RelativeLayout>
    <TextView android:layout_gravity="bottom" android:background="@color/upload_work_edittext_color" android:layout_width="fill_parent" android:layout_height="@dimen/line_height" />
    <RelativeLayout android:layout_width="fill_parent" android:layout_height="@dimen/menu_item_child_height">
        <TextView android:textSize="@dimen/menu_user_name_textsize" android:textColor="@color/home_user_name_color" android:gravity="center_vertical" android:id="@id/item_contact_tel_textview" android:background="@color/white" android:paddingLeft="@dimen/general_space" android:layout_width="wrap_content" android:layout_height="@dimen/menu_item_child_height" android:text="手机：" />
        <EditText android:textSize="@dimen/menu_user_name_textsize" android:textColor="@color/home_user_name_color" android:textColorHint="@color/upload_work_edittext_color" android:id="@id/item_contact_tel_editText" android:background="@color/white" android:layout_width="fill_parent" android:layout_height="@dimen/menu_item_child_height" android:hint="请输入手机号" android:singleLine="true" android:maxLength="11" android:layout_toRightOf="@id/item_contact_tel_textview" android:inputType="phone" />
    </RelativeLayout>
    <TextView android:layout_gravity="bottom" android:background="@color/upload_work_edittext_color" android:layout_width="fill_parent" android:layout_height="@dimen/line_height" />
    <RelativeLayout android:layout_width="fill_parent" android:layout_height="@dimen/menu_item_child_height">
        <TextView android:textSize="@dimen/menu_user_name_textsize" android:textColor="@color/home_user_name_color" android:gravity="center_vertical" android:id="@id/item_contact_tel_textview" android:background="@color/white" android:paddingLeft="@dimen/general_space" android:layout_width="wrap_content" android:layout_height="@dimen/menu_item_child_height" android:text="验证码：" />
        <EditText android:textSize="@dimen/menu_user_name_textsize" android:textColor="@color/home_user_name_color" android:textColorHint="@color/upload_work_edittext_color" android:id="@id/item_verifi_code_editText" android:background="@color/white" android:layout_width="fill_parent" android:layout_height="@dimen/menu_item_child_height" android:hint="请输入验证码" android:singleLine="true" android:maxLength="11" android:layout_toRightOf="@id/item_contact_tel_textview" android:inputType="phone" />
        <Button android:textSize="@dimen/menu_user_name_textsize" android:textColor="@color/white" android:id="@id/item_get_verifi_btn" android:background="@drawable/button_corner_bg" android:paddingLeft="5.0dip" android:paddingTop="3.0dip" android:paddingRight="5.0dip" android:paddingBottom="3.0dip" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="3.0dip" android:text="获取验证码" android:layout_alignParentRight="true" android:layout_centerVertical="true" />
    </RelativeLayout>
    <TextView android:layout_gravity="bottom" android:background="@color/upload_work_edittext_color" android:layout_width="fill_parent" android:layout_height="@dimen/line_height" />
    <RelativeLayout android:background="@color/white" android:layout_width="fill_parent" android:layout_height="@dimen/menu_item_child_height">
        <ImageView android:id="@id/item_price_arrow" android:layout_width="@dimen/arrow_layout_width" android:layout_height="fill_parent" android:layout_marginRight="@dimen/general_space" android:src="@drawable/item_arrow" android:layout_alignParentRight="true" android:layout_centerVertical="true" />
        <TextView android:textSize="@dimen/menu_user_name_textsize" android:textColor="@color/home_user_name_color" android:gravity="right|center" android:id="@id/item_book_date_textview" android:background="@color/white" android:layout_width="@dimen/item_right_text_view_width" android:layout_height="@dimen/menu_item_child_height" android:text="未选择" android:layout_toLeftOf="@id/item_price_arrow" />
        <Button android:textSize="@dimen/menu_user_name_textsize" android:textColor="@color/home_user_name_color" android:gravity="left|center" android:id="@id/item_book_date_btn" android:background="@null" android:paddingLeft="@dimen/general_space" android:layout_width="fill_parent" android:layout_height="fill_parent" android:text="选择拍摄日期" />
    </RelativeLayout>
    <TextView android:layout_gravity="bottom" android:background="@color/upload_work_edittext_color" android:layout_width="fill_parent" android:layout_height="@dimen/line_height" />
    <FrameLayout android:id="@id/item_name_layout" android:background="@color/white" android:layout_width="fill_parent" android:layout_height="wrap_content">
        <TextView android:textSize="@dimen/menu_user_name_textsize" android:textColor="@color/home_user_name_color" android:gravity="center_vertical" android:layout_gravity="left" android:background="@color/white" android:paddingLeft="@dimen/general_space" android:layout_width="wrap_content" android:layout_height="@dimen/menu_item_child_height" android:text="备注" />
        <EditText android:textSize="@dimen/menu_user_name_textsize" android:textColor="@color/home_user_name_color" android:textColorHint="@color/upload_work_edittext_color" android:layout_gravity="right" android:id="@id/item_mark_editText" android:background="@color/white" android:paddingLeft="@dimen/general_space" android:layout_width="@dimen/menu_width" android:layout_height="@dimen/menu_item_child_height" android:hint="预约备注" android:singleLine="true" />
    </FrameLayout>
    <TextView android:layout_gravity="bottom" android:background="@color/upload_work_edittext_color" android:layout_width="fill_parent" android:layout_height="@dimen/line_height" />
    <TextView android:layout_gravity="bottom" android:background="@color/upload_work_edittext_color" android:layout_width="fill_parent" android:layout_height="@dimen/line_height" android:layout_marginTop="@dimen/select_item_category_margin_top" />
    <RelativeLayout android:background="@color/white" android:paddingBottom="@dimen/general_space" android:layout_width="fill_parent" android:layout_height="wrap_content">
        <ImageView android:id="@id/item_simple_imageV" android:background="@drawable/home_photographer_work_default_icon" android:layout_width="@dimen/order_item_simple_shape" android:layout_height="@dimen/order_item_simple_shape" android:layout_margin="@dimen/general_space" />
        <TextView android:textSize="@dimen/item_service_des_textsize" android:textColor="@color/home_user_name_color" android:id="@id/item_title_textV" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/general_space" android:layout_marginRight="@dimen/general_space" android:text="肯定靠感觉快点给店看看" android:layout_toRightOf="@id/item_simple_imageV" />
        <TextView android:textSize="@dimen/photographer_gallery_price_textsize" android:textColor="@color/menu_credit_color" android:id="@id/item_price_textV" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/general_space" android:text="888" android:layout_toRightOf="@id/item_simple_imageV" android:layout_below="@id/item_title_textV" />
        <TextView android:textSize="@dimen/order_item_book_textsize" android:textColor="@color/menu_item_color" android:id="@id/item_price_unit_textV" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="元/人" android:layout_toRightOf="@id/item_simple_imageV" android:layout_below="@id/item_price_textV" />
        <TextView android:textSize="@dimen/photographer_gallery_price_textsize" android:textColor="@color/home_user_name_color" android:id="@id/item_price_textV" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/general_space" android:text="X" android:layout_below="@id/item_title_textV" android:layout_centerHorizontal="true" />
        <Button android:textSize="@dimen/home_user_num_icon_shape" android:textColor="@color/home_user_name_color" android:id="@id/item_book_add_btn" android:background="@drawable/user_btn_add" android:layout_width="@dimen/book_item_add_btn_width" android:layout_height="@dimen/book_item_add_btn_height" android:layout_marginTop="@dimen/general_space" android:layout_marginRight="@dimen/general_space" android:layout_below="@id/item_title_textV" android:layout_alignParentRight="true" />
        <TextView android:textSize="@dimen/home_user_num_icon_shape" android:textColor="@color/home_user_name_color" android:gravity="center" android:id="@id/item_num_book_textV" android:background="@drawable/user_frame_number" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/general_space" android:layout_marginRight="@dimen/general_space" android:text="1" android:layout_toLeftOf="@id/item_book_add_btn" android:layout_below="@id/item_title_textV" android:layout_centerVertical="true" />
        <Button android:textSize="@dimen/home_user_num_icon_shape" android:textColor="@color/home_user_name_color" android:id="@id/item_book_decrease_btn" android:background="@drawable/user_btn_minus" android:layout_width="@dimen/book_item_add_btn_width" android:layout_height="@dimen/book_item_add_btn_height" android:layout_marginTop="@dimen/general_space" android:layout_marginRight="@dimen/general_space" android:layout_toLeftOf="@id/item_num_book_textV" android:layout_below="@id/item_title_textV" android:layout_centerVertical="true" />
    </RelativeLayout>
    <TextView android:layout_gravity="bottom" android:background="@color/upload_work_edittext_color" android:layout_width="fill_parent" android:layout_height="@dimen/line_height" />
    <RelativeLayout android:background="@color/white" android:layout_width="fill_parent" android:layout_height="@dimen/menu_item_child_height">
        <TextView android:textSize="@dimen/home_user_num_icon_shape" android:textColor="@color/home_user_name_color" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/general_space" android:text="预约订单总价" android:layout_centerVertical="true" />
        <TextView android:textSize="@dimen/home_user_num_icon_shape" android:textColor="@color/nav_btn_textcolor" android:id="@id/item_book_total_price_textV" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/general_space" android:text="10000元" android:layout_alignParentRight="true" android:layout_centerVertical="true" />
    </RelativeLayout>
    <TextView android:layout_gravity="bottom" android:background="@color/upload_work_edittext_color" android:layout_width="fill_parent" android:layout_height="@dimen/line_height" />
    <Button android:layout_gravity="center_horizontal" android:id="@id/item_book_btn" android:background="@drawable/user_btn_reservation" android:layout_width="@dimen/order_cancle_book_btn_width" android:layout_height="@dimen/nav_bar_height" android:layout_marginTop="@dimen/photographer_btn_margin_top" />
</LinearLayout>